{
  "name": "@mwohaji/app",
  "license": "Apache-2.0",
  "version": "1.0.2",
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22"
  },
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device \"Namgunghyun13mini\"",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:device:release": "react-native run-ios --device \"Namgunghyun13mini\" --configuration Release",
    "ios:bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios:clean:force": "rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "pod": "react-native setup-ios-permissions && pod-install",
    "pod:clean": "cd ios && rm -rf Pods && rm Podfile.lock && pod deintegrate && pod setup && pod repo update && cd ..",
    "pod:clean:force": "sudo rm -rf ~/Library/Caches/CocoaPods",
    "android": "adb devices && react-native run-android",
    "android:release": "adb devices && react-native run-android --mode=release",
    "android:build": "react-native build-android --mode=release",
    "android:clean": "rm -rf android/build && sudo rm -rf android/app/build && sudo rm -rf android/.gradle",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "postversion": "react-native-version --never-amend",
    "codepush": "yarn codepush:android && yarn codepush:ios",
    "codepush:android": "appcenter codepush release-react -a mwohaji/mwohaji-android -d Staging",
    "codepush:ios": "appcenter codepush release-react -a mwohaji/mwohaji-ios -d Staging --plist-file ios/mwohaji/Info.plist",
    "test:coverage": "yarn test -- --coverage --watchAll=false",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint . --fix && yarn typecheck",
    "lint:ci": "eslint . --max-warnings=0 && yarn typecheck",
    "splash": "yarn react-native generate-bootsplash assets/icon.png",
    "localip": "ifconfig | grep 'inet ' | grep -Fv 127.0.0.1 | awk '{print $2}'",
    "deeplink:ios": "npx uri-scheme open mwohaji://user/test --ios",
    "deeplink:android": "npx uri-scheme open mwohaji://user/test --ios"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.7",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@jest/globals": "^29.6.2",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/datetimepicker": "^7.4.1",
    "@react-native-firebase/analytics": "^18.2.0",
    "@react-native-firebase/app": "^18.2.0",
    "@react-native-firebase/auth": "^18.3.0",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-native-picker/picker": "^2.5.0",
    "@react-native-seoul/kakao-login": "^5.3.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@tanstack/react-query": "^4.29.19",
    "@trpc/client": "^10.34.0",
    "@trpc/react-query": "^10.34.0",
    "@trpc/server": "^10.34.0",
    "axios": "^1.4.0",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-bootsplash": "^4.7.5",
    "react-native-code-push": "^8.0.2",
    "react-native-device-info": "^10.8.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-in-app-review": "^4.3.3",
    "react-native-linear-gradient": "^2.7.3",
    "react-native-maps": "^1.7.1",
    "react-native-mmkv": "^2.10.1",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^17.0.0",
    "react-native-permissions": "^3.8.4",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^9.2.0",
    "react-native-wheel-picker-android": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/gradle-plugin": "^0.72.11",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "pod-install": "^0.1.38",
    "prettier": "^2.4.1",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "reactNativePermissionsIOS": [
    "LocationWhenInUse"
  ]
}
